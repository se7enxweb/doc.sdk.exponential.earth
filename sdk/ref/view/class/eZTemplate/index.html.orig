<span class="dox_keyword">class </span>mytest
{
    function mytest( $n, $s )
    {
        $this-&gt;n = $n;
        $this-&gt;s = $s;
    }

    function hasAttribute( $attr )
    {
        <span class="dox_keywordflow">return</span> ( $attr == <span class="dox_stringliteral">"name"</span> || $attr == <span class="dox_stringliteral">"size"</span> );
    }

    function &amp;attribute( $attr )
    {
        <span class="dox_keywordflow">switch</span> ( $attr )
        {
            <span class="dox_keywordflow">case</span> <span class="dox_stringliteral">"name"</span>;
            <span class="dox_keywordflow">return</span> $this-&gt;n;
            <span class="dox_keywordflow">case</span> <span class="dox_stringliteral">"size"</span>;
            <span class="dox_keywordflow">return</span> $this-&gt;s;
            <span class="dox_keywordflow">default</span>:
                <span class="dox_keywordflow">return</span> null;
        }
    }

};

