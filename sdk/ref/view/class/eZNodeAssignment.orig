                  <span class="dox_stringliteral">"function_attributes"</span> =&gt; array( <span class="dox_stringliteral">"current"</span> =&gt; <span class="dox_stringliteral">"currentVersion"</span>,
                                                  <span class="dox_stringliteral">"class_name"</span> =&gt; <span class="dox_stringliteral">"className"</span> ),
                  <span class="dox_stringliteral">"increment_key"</span> =&gt; <span class="dox_stringliteral">"id"</span>,
                  <span class="dox_stringliteral">"class_name"</span> =&gt; <span class="dox_stringliteral">"eZContentClass"</span>,
